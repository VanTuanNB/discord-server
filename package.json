{
    "name": "maple-metting",
    "module": "index.ts",
    "type": "commonjs",
    "scripts": {
        "dev": "bun --env-file=.env --hot ./src/index.ts",
        "docker": "docker run -d -p 5000:5000 bun-express",
        "build": "bun build ./src/index.ts --outdir ./dist --target=bun",
        "test": "bun test --preload ./src/index.ts",
        "production": "bun --watch ./dist/index.js"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "tsc-alias": "^1.8.8"
    },
    "peerDependencies": {
        "typescript": "^5.0.0"
    },
    "dependencies": {
        "@types/express": "^4.17.21",
        "express": "^4.18.2"
    },
    "paths": {
        "@/*": [
            "dist/*"
        ]
    }
}
