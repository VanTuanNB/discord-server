{
  "name": "maple-metting",
  "module": "./src/index.ts",
  "type": "commonjs",
  "scripts": {
    "dev": "bun --env-file=.env --hot ./src/index.ts",
    "docker": "docker run -d -p 5000:5000 bun-express",
    "build": "bun build ./src/index.ts --outdir ./dist --target=bun",
    "test": "bun test --preload ./src/index.ts",
    "production": "bun --watch ./dist/index.js"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "tsc-alias": "^1.8.8"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  },
  "paths": {
    "@/*": [
      "dist/*"
    ]
  }
}
